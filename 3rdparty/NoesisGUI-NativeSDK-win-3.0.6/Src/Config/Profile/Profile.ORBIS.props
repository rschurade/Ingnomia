<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <_PropertySheetDisplayName>Noesis Profile</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <OptimizationLevel>Level3</OptimizationLevel>
      <FastMath>true</FastMath>
      <PreprocessorDefinitions>NDEBUG;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <AdditionalOptions>-fno-sce-stack-sizes-section -fno-asynchronous-unwind-tables -fmerge-all-constants %(AdditionalOptions)</AdditionalOptions>
    </ClCompile>
    <Link>
      <InfoStripping>StripSymsAndDebug</InfoStripping>
      <DataStripping>StripFuncsAndData</DataStripping>
      <DuplicateStripping>true</DuplicateStripping>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>